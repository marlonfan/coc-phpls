{
  "name": "coc-phpls",
  "version": "1.0.2",
  "description": "PHP language server extension for coc.nvim",
  "main": "lib/index.js",
  "publisher": "marlonfan",
  "engines": {
    "coc": "^0.0.15"
  },
  "keywords": [
    "coc.nvim",
    "php",
    "phpls"
  ],
  "scripts": {
    "clean": "rimraf lib",
    "build": "tsc -p tsconfig.json",
    "prepare": "yarn clean && yarn build"
  },
  "activationEvents": [
    "onLanguage:php"
  ],
  "contributes": {
    "configuration": {
      "title": "php configuration",
      "properties": {
        "phpls.path": {
          "type": "string",
          "default": "",
          "description": "lsp server path"
        },
        "phpls.enable": {
          "type": "boolean",
          "default": true,
          "description": "is enable php lsp"
        }
      }
    }
  },
  "author": "marlon@tfan.net",
  "license": "MIT",
  "devDependencies": {
    "@chemzqm/tsconfig": "^0.0.3",
    "@chemzqm/tslint-config": "^1.0.17",
    "@types/fs-extra": "^5.0.4",
    "@types/glob": "^7.1.1",
    "@types/node": "^10.12.11",
    "coc.nvim": "^0.0.32",
    "rimraf": "^2.6.2",
    "tslint": "^5.11.0",
    "typescript": "^3.2.1"
  },
  "dependencies": {
    "fs-extra": "^7.0.1",
    "glob": "^7.1.3",
    "intelephense-server": "^0.8.8",
    "tslib": "^1.9.3",
    "vscode-languageserver-protocol": "^3.13.0"
  }
}
